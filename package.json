{
  "name": "cloui",
  "version": "0.0.0-development",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.development next dev --turbo",
    "test": "dotenv -e .env.test next './src/test' jest",
    "build": "next build",
    "start": "dotenv -e .env.production next start",
    "cypress": "cypress open",
    "test:e2e": "playwright './src/test'",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint './src/**/styled.ts' './src/**/*.tsx'",
    "format": "prettier --write \"./src/**/*.+(js|mjs|jsx|ts|mts|tsx|json|yml|yaml|css|less|scss|md|mdx)\"",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx}": [
      "eslint --cache"
    ],
    "styled.ts": [
      "stylelint --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run check:type && npm run format"
    }
  },
  "dependencies": {
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-stylelint": "^20.0.0",
    "eslint-formatter-gitlab": "^5.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "globals": "^13.21.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "next": "13.4.19",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "prettier-stylelint": "^0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^4.0.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "swiper": "^10.2.0",
    "typescript": "5.2.2",
    "url": "^0.11.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "cypress": "^13.1.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "semantic-release": "^21.1.1",
    "storybook": "^7.4.0",
    "ts-jest": "^29.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leoclao/cloui-nextjs.git"
  }
}
