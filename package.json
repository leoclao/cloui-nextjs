{
  "name": "cloui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.development next dev",
    "test": "dotenv -e .env.test next test",
    "build": "next build",
    "start": "dotenv -e .env.production next start",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint 'src/**/styled.ts' 'src/**/*.tsx'",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|md|mdx)\"",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache"
    ],
    "styled.ts": [
      "stylelint --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run check:type && npm run format"
    }
  },
  "dependencies": {
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "globals": "^13.21.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "next": "13.4.19",
    "prettier": "^3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.66.1",
    "storybook": "^7.3.2",
    "stylelint": "^15.10.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "swiper": "^10.2.0",
    "typescript": "5.1.6",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0"
  }
}
